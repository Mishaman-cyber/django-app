{
    "version": 3.9,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "installCommand": "pip install --no-cache-dir --target . --upgrade -r requirements.txt"

        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproj.settings",
      "PYTHONUNBUFFERED": "1",
      "PYTHONPATH": "."
    }
  }
  
